grammar Wiki
{
    options
    {
		Start = Sums;
        Whitespace = ws;
    }
    terminals
    {
        int : ('0' .. '9')+
            ;
        id  : ('a' .. 'z')+
            ;
        ws  : ' '
            ;
    }
    rules
    {
		Sums
            : {Sum: Sums '+' Products}
			| Products
            ;

        Products
            : {Prod: Products '*' Value}
            | Value
            ;

        Value
            : int
            | id
            ;
    }
}
