grammar Wiki
{
    options
    {
		Start = Sums;
    }
    terminals
    {
        int : (0 .. 9)*
        id  | (a .. z)*
    }
    rules
    {
		Sums
            : Sums '+' Products
			| Products
            ;

        Products
            : Products '*' Value
            | Value
            ;

        Value
            : int
            | id
            ;
    }
}
