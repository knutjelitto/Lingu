grammar Expression
{
    options
    {
    }
    terminals
    {
        lineComment2
			: '//' (.* - newline)* newline
            ;

        lineComment
			: '//' ~(.* newline .*) newline
            ;

        newline
            : '\n'
            | '\r\n'
            ;
    }
    rules
    {
    }
}
